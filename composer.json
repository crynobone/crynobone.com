{
    "name": "crynobone/website",
    "description": "Source code for crynobone.com",
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": ">=5.6.0",
        "orchestra/foundation": "~3.1.15",
        "orchestra/installer": "~3.1",
        "orchestra/control": "~3.1",
        "orchestra/story": "~3.1",
        "orchestra/avatar": "~3.1"
    },
    "require-dev": {
        "orchestra/testing": "~3.1",
        "phpspec/phpspec": "~2.1",
        "stackie/medium-theme": "~1.0",
        "stackie/adminlte-theme": "~1.0"
    },
    "autoload": {
        "classmap": [
            "resources/database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan orchestra:assemble"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan orchestra:assemble"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
